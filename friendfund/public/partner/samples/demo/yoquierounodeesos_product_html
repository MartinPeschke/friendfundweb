jQuery(document).ready(function() {
	jQuery('#ratingSubmit').click(function(){
		if(jQuery('#acepto').attr('checked')===false){
			alert('Debes aceptar la pol\u00EDtica de privacidad.');
			return (false);
		}
	});
	jQuery('.reviewPopUpSubmit').click(function() {
		jQuery('#popupContact').slideDown();
	});
 
	jQuery('#popupContactClose').click(function() {
		jQuery('#popupContact').slideUp();
	});
	jQuery('.ratings-producto .reviewPopUpSubmit').click(function(){
		var targetOffset = jQuery('.yqudereview').offset().top;
		jQuery('html,body').animate({scrollTop: targetOffset}, 1000);
	});
});
