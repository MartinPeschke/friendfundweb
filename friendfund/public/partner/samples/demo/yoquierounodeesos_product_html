    jQuery('#searchPriceByCat').change(function(){
        try{
            var range = jQuery('#searchPriceByCat')[0].options[jQuery('#searchPriceByCat')[0].selectedIndex].value;
			if(range != '0-0'){
				var arrayOfStrings = range.split('-');
				document.getElementById('price_from').setAttribute("value",arrayOfStrings[0]);
				document.getElementById('price_to').setAttribute("value",arrayOfStrings[1]);
				document.forms["byPrice"].submit();
			}
        }
        catch(exception){
        }
 
    });
